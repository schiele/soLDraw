EXTRA_FLAGS :=
OPENSCAD = openscad --hardwarnings --export-format binstl -Dpart='"$*"' $(EXTRA_FLAGS) -o $@ $<

PARTS := 3001 3002 3003 3004 3005 3006 3007 3008 3009 3010 3011 3020 3021 3022 3023 3024 3026 3027 3028 3029 3030 3031 3032 3033 3034 3035 3036 3037 3038 3039 3040 3068b 3623 3641 3710 3788 3821 3822 3823 3828 3829a 3937 3938 4070 4079 4213 4214 4215 4315 4600 4624 6141

all: $(foreach p,$(PARTS),$(p).stl)

%.stl: soLDraw.scad
	$(OPENSCAD)

.PHONY: clean

clean:
	rm -f *.stl
